{
  "name": "express-rabbitmq-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "db:migrate": "npm run build && sequelize-cli db:migrate",
    "db:seed": "npm run build && sequelize-cli db:seed:all",
    "start-express": "ts-node src/index.ts",
    "start-producer": "ts-node src/services/mqproducer.ts",
    "start-consumer": "ts-node src/services/mqconsumer.ts",
    "start-all": "concurrently \"npm run start-express\" \"npm run start-producer\" \"npm run start-consumer\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/sequelize": "^4.28.20",
    "@types/ws": "^8.5.10",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "ws": "^8.17.1"
  }
}
